# Database Configuration
POSTGRES_USER=superset
POSTGRES_PASSWORD=superset
POSTGRES_DB=marketing_analytics
SUPERSET_DB_URI=postgresql://superset:superset@postgres:5432/marketing_analytics

# Redis Configuration
REDIS_URL=redis://redis:6379/0

# Superset Security
SECRET_KEY=your_secure_key_here

# OAuth Configuration (for Google auth)
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret

# Email Configuration
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your-email@gmail.com
SMTP_PASSWORD=your-app-password
SMTP_MAIL_FROM=reports@yourdomain.com

# AWS Deployment (for CI/CD)
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_REGION=us-west-2
RDS_ENDPOINT=your-db-instance.rds.amazonaws.com
REDIS_ENDPOINT=your-redis-instance.cache.amazonaws.com

# Slack Notifications (for CI/CD)
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/your/webhook/url 